// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

//yarn prisma migrate dev
model school {
  id_escola          Int    @id
  nu_ano_saeb        Int
  co_uf              Int
  sg_uf              String
  no_uf              String
  co_municipio       Int
  no_municipio       String
  no_escola          String
  tp_tipo_rede       Int
  tp_localizacao     Int
  tp_capital         Int
  qtd_alunos_inse    Int
  media_inse         Float
  inse_classificacao String
  pc_nivel_1         Float
  pc_nivel_2         Float
  pc_nivel_3         Float
  pc_nivel_4         Float
  pc_nivel_5         Float
  pc_nivel_6         Float
  pc_nivel_7         Float
  pc_nivel_8         Float

  @@map("schools")
}
